<p id="notice"><%= notice %></p>

<p>
  <strong>Project:</strong>
  <%= @backlink.project %>
</p>

<p>
  <strong>Project url:</strong>
  <%= @backlink.project_url %>
</p>

<p>
  <strong>Backlink url:</strong>
  <%= @backlink.backlink_url %>
</p>

<p>
  <strong>Method:</strong>
  <%= @backlink.method %>
</p>

<p>
  <strong>Published at:</strong>
  <%= @backlink.published_at.strftime('%d/%m/%Y') if @backlink.published_at %>
</p>

<p>
  <strong>Notes:</strong>
  <%= @backlink.notes %>
</p>

<p>
<%= link_to 'Edit', edit_backlink_path(@backlink) %> | <%= link_to 'Delete', @backlink, method: :delete, data: { confirm: 'Are you sure?' } %>
</p>

<p>
  <%= link_to '<< Back', backlinks_path %>  
</p>


